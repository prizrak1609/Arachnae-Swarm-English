              <first Class="ColorGenerator_Options">
                <options>
                  <li>
                    <weight>15</weight>
                    <min>(0.4,0.3,0.5)</min>
                    <max>(0.6,0.1,0.7)</max>
                  </li>
                  <li>
                    <weight>6</weight>
                    <min>(0.9,0.9,0.9)</min>
                    <max>(0.9,0.9,0.9)</max>
                  </li>
                  <li>
                    <weight>6</weight>
                    <min>(1,0.8,0.8)</min>
                    <max>(1,0.9,0.9)</max>
                  </li>
                  <li>
                    <weight>3</weight>
                    <min>(1,1,1)</min>
                    <max>(1,1,1)</max>
                  </li>
                </options>
              </first>
            </li>
          </colorChannels>
          <!-- 额外身体部件 -->
          <bodyAddons>
          </bodyAddons>
          <!-- 图像放大 -->
          <borderScale>4</borderScale>
          <atlasScale>4</atlasScale>
          <customDrawSize>(4,4)</customDrawSize>
          <customHeadDrawSize>(1.0,1.0)</customHeadDrawSize>
          <customPortraitDrawSize>(2,2)</customPortraitDrawSize>
        </alienPartGenerator>
        <growthAges Inherit="False">
          <li>0</li>
